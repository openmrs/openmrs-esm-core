{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "es2020",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "strictNullChecks": true,
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "lib": ["dom", "es2022"],
    "skipLibCheck": true
  }
}
