{
  "name": "@openmrs/esm-feature-flags",
  "version": "5.0.2",
  "license": "MPL-2.0",
  "description": "A feature flag system for the OpenMRS Single-Spa framework.",
  "browser": "dist/openmrs-esm-feature-flags.js",
  "main": "src/index.ts",
  "source": true,
  "scripts": {
    "test": "jest --config jest.config.js --passWithNoTests",
    "build": "webpack --mode=production",
    "build:development": "webpack --mode development",
    "analyze": "webpack --mode=production --env analyze=true",
    "typescript": "tsc",
    "lint": "eslint src --ext ts,tsx"
  },
  "keywords": [
    "openmrs",
    "microfrontends"
  ],
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-core.git"
  },
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-core/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-core#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "peerDependencies": {
    "@openmrs/esm-globals": "5.x",
    "@openmrs/esm-state": "5.x",
    "single-spa": "5.x"
  },
  "devDependencies": {
    "@openmrs/esm-globals": "^5.0.2",
    "@openmrs/esm-state": "^5.0.2"
  }
}
